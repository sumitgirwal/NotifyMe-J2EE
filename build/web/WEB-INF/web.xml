<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>sendMail</servlet-name>
        <servlet-class>com.nmp.sendMail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>com.nmp.signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>checkwho</servlet-name>
        <servlet-class>com.nmp.checkwho</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loadDB</servlet-name>
        <servlet-class>com.nmp.loadDB</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sendMail2</servlet-name>
        <servlet-class>com.nmp.sendMail2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>sendMail</servlet-name>
        <url-pattern>/sendMail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>checkwho</servlet-name>
        <url-pattern>/checkwho</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loadDB</servlet-name>
        <url-pattern>/loadDB</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sendMail2</servlet-name>
        <url-pattern>/sendMail2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            300
        </session-timeout>
    </session-config>
</web-app>
